{
    "name": "ALREADweets  ",
    "version": "1.0", 
    "manifest_version": 1,
    "description": "Notifies you when you reach tweets that you have already read.",
    "content_scripts": [
        {
            "matches": ["https://twitter.com/*"],
            "js" : ["content.js"],
            "run_at": "document_end"
        }
    ]


}
